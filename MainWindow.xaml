        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:slightly_cooler_sound_board"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ToolBarTray HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Width="431">
            <ToolBar Band="1" BandIndex="1">
                <Button x:Name="FolderSelect" Click="Button_Click" Background="White">
                    Select folder
                </Button>
            </ToolBar>
        </ToolBarTray>
        <!--<GroupBox Header="blocks go here" Margin="0,42,161,129" AutomationProperties.Name="MotherBox">
            
        </GroupBox>-->
        
            <ScrollViewer Margin="0,82,0,0">

            <StackPanel x:Name="stackPanel" AutomationProperties.Name="StackThatHoldsEverythings" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        </ScrollViewer>
    </Grid>
</Window>
